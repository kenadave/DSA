private boolean checkUniqueCharString(String str){
	        if(str.length()>26)return false;
	        int[] freq=new int[26];
	        char[] arr = str.toCharArray();
	        for(char ch:arr){
	            freq[ch-'a']++;
	            if(freq[ch-'a']>1){
	                return false;
	            }
	        }
	        System.out.println('a'-'a'); // 0
	        System.out.println('a'-'0'); // 49
	        System.out.println('a'); //a
	        return true;
	    }
